<%= form_with model: @question  do |f| %>
  <div>
   <%= f.label :title  %>
   <%= f.text_field :title, required: true, minlenght: 2, placeholder: "Title"  %>
  </div>
  <div>
   <%= f.label :body  %>
   <%= f.text_area :body, required: true, minlenght: 2  %>
  </div>
  <div>
    <%= f.submit "Submit" %>
  </div>
<% end %>